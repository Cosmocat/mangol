        "@angular/forms": "^8.2.0",
        "@angular/http": "^7.2.15",
        "@angular/material": "^8.1.2",
        "@angular/platform-browser": "^8.2.0",
        "@angular/platform-browser-dynamic": "^8.1.3",
        "@angular/router": "^8.2.0",
        "@ngrx/effects": "^8.2.0",
